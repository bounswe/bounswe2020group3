{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "warn",
        "consistent-return": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "warn",
        "no-irregular-whitespace": "off",
        "linebreak-style": "off"
    }
}
